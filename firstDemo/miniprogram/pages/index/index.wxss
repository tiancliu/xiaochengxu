/**index.wxss**/
page{
  background: #b3d4db;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container image{
  width: 128rpx;
  height: 128rpx;
  margin-top: 160rpx;
}
.title{
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 100rpx;
}
.btn{
  border: 1px solid #405f80;
  width: 320rpx;
  height: 80rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  margin-top: 200rpx;
}
.btn .open{
  font-size: 26rpx;
  color: #405f80;
}
